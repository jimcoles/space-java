/*
  The Hello, World for Space is Hello, Space!
*/
(space-defn
    (set name HelloSpaceNorm) // set space name
    (access public)
    (method main
        (arguments
            (argument
                (name args)  // name or arg
                (space relation
                    (type string)
                )
            )
        )
        (body
            (action-sequence
                (sys-print "Hello, Space ~!@#$%^&*()-+!")
                (sys-print "\"")
                (sys-barf)
                (tri-guy one two three four)
                (add 1 2 3.0)
            )
        )
    )
)